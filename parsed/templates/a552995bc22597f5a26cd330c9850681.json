{"components/cms/lookbook":"<div class=\"section-spacing\">\r\n    <div class=\"LookBook_Section_BackGround\">\r\n        <div class=\"container\">\r\n            <div class=\"home_most_popular_products\">\r\n                <div class=\"myheadeing\">\r\n                    <h2 class=\"page-heading\">\r\n                    {{theme_settings.lookbook-heading}}\r\n                    <p>{{theme_settings.subheading-lookbook}}</p>\r\n                    <span class=\"head-line\"></span>\r\n                    </h2>\r\n                </div>\r\n                <div class=\"mp_parent_slider\" \r\n                    data-slick='{\r\n                        \"arrows\": true,\r\n                        \"dots\": false,\r\n                        \"slidesToShow\": 1,\r\n                        \"slidesToScroll\": 1,\r\n                        \"prevArrow\": \".js-product-prev-arrow-lookbook\",\r\n                        \"nextArrow\": \".js-product-next-arrow-lookbook\"\r\n                    }' >\r\n\r\n                \r\n                    <!-- Slider 1 -->\r\n                    <div class=\"mp_product_list\">\r\n                        <div class=\"mp_inner\">\r\n                            <div class=\"image_section\">\r\n                                <img class=\"img-fluid lazyload\" src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.mp_slider_1_image}}\" alt=\"Most Popular\" width=\"auto\" height=\"auto\" />\r\n                                {{#each (split theme_settings.mp_slider_1_pIdsDots \",\") as |productDots| }}\r\n                                    {{#each (split ../theme_settings.mp_slider_1_pIds \",\") as |productIds| }}\r\n                                        {{#if @../index '==' @index}}\r\n                                            <span data-id=\"{{productIds}}\" class=\"dots {{#if @index '==' '0'}}active{{/if}}\" data-index=\"{{@index}}\" style=\"top:{{first (split productDots '-')}};left:{{last (split productDots '-')}}\" ></span>\r\n                                        {{/if}}\r\n                                    {{/each}}\r\n                                {{/each}}\r\n                            </div>\r\n                            <div class=\"product_section mp-pro-data-1\"></div>\r\n                            <input type=\"hidden\" id=\"mp_slider_1_data\" value=\"{{theme_settings.mp_slider_1_pIds}}\" />\r\n                        </div>\r\n                    </div>\r\n\r\n                    <!-- Slider 2 -->\r\n                    <div class=\"mp_product_list\">\r\n                        <div class=\"mp_inner\">\r\n                            <div class=\"image_section\">\r\n                                <img class=\"img-fluid lazyload\" src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.mp_slider_2_image}}\" alt=\"Most Popular\" width=\"auto\" height=\"auto\" />\r\n                                {{#each (split theme_settings.mp_slider_2_pIdsDots \",\") as |productDots| }}\r\n                                    {{#each (split ../theme_settings.mp_slider_2_pIds \",\") as |productIds| }}\r\n                                        {{#if @../index '==' @index}}\r\n                                            <span data-id=\"{{productIds}}\" class=\"dots {{#if @index '==' '0'}}active{{/if}}\" data-index=\"{{@index}}\" style=\"top:{{first (split productDots '-')}};left:{{last (split productDots '-')}}\" ></span>\r\n                                        {{/if}}\r\n                                    {{/each}}\r\n                                {{/each}}\r\n                            </div>\r\n                            <div class=\"product_section mp-pro-data-2\"></div>\r\n                            <input type=\"hidden\" id=\"mp_slider_2_data\" value=\"{{theme_settings.mp_slider_2_pIds}}\" />\r\n                        </div>\r\n                    </div>\r\n\r\n                    <!-- Slider 3 -->\r\n                    <div class=\"mp_product_list\">\r\n                        <div class=\"mp_inner\">\r\n                            <div class=\"image_section\">\r\n                                <img class=\"img-fluid lazyload\" src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.mp_slider_3_image}}\" alt=\"Most Popular\" width=\"auto\" height=\"auto\" />\r\n                                {{#each (split theme_settings.mp_slider_3_pIdsDots \",\") as |productDots| }}\r\n                                    {{#each (split ../theme_settings.mp_slider_3_pIds \",\") as |productIds| }}\r\n                                        {{#if @../index '==' @index}}\r\n                                            <span data-id=\"{{productIds}}\" class=\"dots {{#if @index '==' '0'}}active{{/if}}\" data-index=\"{{@index}}\" style=\"top:{{first (split productDots '-')}};left:{{last (split productDots '-')}}\" ></span>\r\n                                        {{/if}}\r\n                                    {{/each}}\r\n                                {{/each}}\r\n                            </div>\r\n                            <div class=\"product_section mp-pro-data-3\"></div>\r\n                            <input type=\"hidden\" id=\"mp_slider_3_data\" value=\"{{theme_settings.mp_slider_3_pIds}}\" />\r\n                        </div>\r\n                    </div>                \r\n                </div>\r\n                <div class=\"all-btn recent-post\">                \r\n                    <button class=\"js-product-prev-arrow-lookbook slick-prev slick-arrow\"><svg width=\"38px\" height=\"26px\"><use xlink:href=\"#arrow-left\"></use></svg></button>\r\n                    <button class=\"js-product-next-arrow-lookbook slick-next slick-arrow\"><svg width=\"38px\" height=\"26px\"><use xlink:href=\"#arrow-right\"></use></svg></button>            \r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n"}
