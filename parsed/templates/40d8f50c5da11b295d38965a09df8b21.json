{"components/common/alert/alert-modal":"<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"alert-icon error-icon\">\r\n        <span class=\"icon-content\">\r\n            <span class=\"line line-left\"></span>\r\n            <span class=\"line line-right\"></span>\r\n        </span>\r\n    </div>\r\n\r\n    <div class=\"alert-icon warning-icon\">\r\n        <div class=\"icon-content\">!</div>\r\n    </div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container\">\r\n        <button type=\"button\" class=\"confirm button\" data-reveal-close>{{lang 'common.ok'}}</button>\r\n        <button type=\"button\" class=\"cancel button\" data-reveal-close>{{lang 'common.cancel'}}</button>\r\n    </div>\r\n</div>\r\n","components/common/body":"<main class=\"body\" id=\"main-content\" role=\"main\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} \r\n    {{/block}}\r\n\r\n    <div class=\"top-bar\" id=\"header_banner\">\r\n        <button id=\"closeButton\"><span>X</span></button> \r\n      <div class=\"container section-gap announcementbar\" >\r\n        <div class=\"row\">\r\n            <div class=\"col-12 text-center\">\r\n                <div class=\"header_bar\">\r\n                    \r\n                    <div class=\"topBarPromotion-carousel\"\r\n                    data-slick='{\r\n                        \"dots\": false,\r\n                        \"arrows\": false,\r\n                        \"infinite\": true,\r\n                        \"mobileFirst\": true,\r\n                        \"vertical\": false,\r\n                        \"autoplay\": true,\r\n                        \"slidesToShow\": 1,\r\n                        \"slidesToScroll\": 1,\r\n                        \"responsive\": [\r\n                            {\r\n                                \"breakpoint\": 768,\r\n                                \"settings\": {\r\n                                    \"arrows\": false\r\n                                }\r\n                            }\r\n                        ]\r\n                    }'\r\n                >       \r\n                        <div class=\"top-bar-f\" aria-hidden=\"true\">\r\n                            <a href=\"{{theme_settings.top_text_link}}\" title=\"offer\">{{theme_settings.top_text}}</a>         \r\n                        </div>\r\n                        <div class=\"top-bar-f\" aria-hidden=\"true\">\r\n                            <a href=\"{{theme_settings.top_text_link2}}\" title=\"offer\">{{theme_settings.top_text2}}</a>              \r\n                        </div>\r\n                </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n      </div>\r\n    </div> \r\n\r\n    <div class=\"\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n</main>\r\n","components/common/modal/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    {{> components/common/modal/modal-close-btn}}\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n","components/common/modal/modal-close-btn":"<button class=\"modal-close\"\r\n        type=\"button\"\r\n        title=\"{{lang 'common.close'}}\"\r\n        {{additional_attr}}\r\n>\r\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\r\n    <span aria-hidden=\"true\">&#215;</span>\r\n</button>\r\n"}
